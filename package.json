{
  "name": "stakinghood",
  "private": false,
  "description": "The \"Robinhood\" for Staking in Validators on the Cosmos network.",
  "version": "0.1.0",
  "author": "@mirshko",
  "dependencies": {
    "@reach/dialog": "^0.1.2",
    "@reach/visually-hidden": "^0.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-styled-components": "^3.0.4",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-hanger": "^1.1.1",
    "react-helmet": "^5.2.0",
    "rebass": "^3.0.0",
    "styled-components": "^4.1.3",
    "typeface-inter": "^3.2.0",
    "unstated": "^2.1.1"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -o",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "start:test": "jest --watchAll"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-gatsby": "^0.1.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-env.js"
  }
}
